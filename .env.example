# Environment Variables Template
# Copy this file to .env and fill in your values
# DO NOT commit .env file - it's in .gitignore

# ============================================
# Backend Environment Variables (Required)
# ============================================

# CHES (Common Hosted Email Service) Configuration
# Get these values from your team's secret management system
CHES_CLIENT_ID=09C5071A-ACE9B6FACF6
CHES_CLIENT_SECRET=your-ches-client-secret-here
CHES_TOKEN_URL=https://test.loginproxy.gov.bc.ca/auth/realms/comsvcauth/protocol/openid-connect/token

# S3 Object Storage Configuration
# Get these values from your team's secret management system
S3_ACCESSKEY=nr-fsa-tst
S3_BUCKETNAME=tivpth
S3_ENDPOINT=nrs.objectstore.gov.bc.ca
S3_SECRETKEY=your-s3-secret-key-here

# Frontend URL for CORS (optional for local development)
# FRONTEND_URL=http://localhost:3000

# ============================================
# Frontend Environment Variables (Required)
# ============================================

# Cognito Authentication Configuration
VITE_MAIN_VERSION=1.0.0
VITE_COGNITO_REGION=ca-central-1
VITE_USER_POOLS_ID=your-cognito-user-pool-id
VITE_USER_POOLS_WEB_CLIENT_ID=your-cognito-client-id
VITE_AWS_DOMAIN=your-cognito-domain.auth.ca-central-1.amazoncognito.com
VITE_ZONE=DEV

# Backend URL (not needed when using Caddy proxy, uses relative URLs)
# VITE_BACKEND_URL=http://localhost:5000
