{
  "name": "nr-results-exam-back",
  "version": "1.0.0",
  "description": "backend service for results exam",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "node scripts/run-tests.mjs",
    "test:coverage": "c8 --check-coverage --reporter=lcov --reporter=text --report-dir=coverage node scripts/run-tests.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "c8": {
    "exclude": [
      "scripts/run-tests.mjs"
    ],
    "check-coverage": true,
    "lines": 70,
    "branches": 70,
    "functions": 70,
    "statements": 70
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^5.0.0",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "minio": "^8.0.0",
    "express-rate-limit": "^8.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "c8": "^10.0.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "nodemon": "^3.0.1",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4"
  }
}
